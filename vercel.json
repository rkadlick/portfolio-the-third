{
	"builds": [
		{
			"src": "src/next.config.js",
			"use": "@vercel/next"
		  },
		  {
			"src": "sanity/package.json",
			"use": "@vercel/static-build",
			"config": { "distDir": "studio-dist" }
		  }
	],
	"routes": [
	  { "src": "/studio/(.*)", "dest": "/studio-dist/$1" },
	  { "src": "/(.*)", "dest": "/src/$1" }
	]
  }